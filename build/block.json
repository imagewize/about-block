{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "imagewize/about-block",
  "version": "0.1.0",
  "title": "About Block",
  "category": "text",
  "icon": "columns",
  "description": "About section with image and text",
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": [
      "wide",
      "full"
    ],
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    }
  },
  "attributes": {
    "backgroundColor": {
      "type": "string",
      "default": "#ebeced"
    },
    "textColor": {
      "type": "string",
      "default": "#98999a"
    },
    "style": {
      "type": "object"
    }
  },
  "textdomain": "about-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}